; Disassembly of "Nectaris GB (Japan) (SGB Enhanced).gb"

DEF OWN EQU $00
DEF FOE EQU $80

DEF TILE_OPEN EQU $00
DEF TILE_ROAD EQU $01
DEF TILE_HILL EQU $02
DEF TILE_DSRT EQU $03
DEF TILE_VLLY EQU $04
DEF TILE_MNTN EQU $05
DEF TILE_BRDG EQU $06
DEF TILE_UBAS EQU $08
DEF TILE_GBAS EQU $0a
DEF TILE_UFAC EQU $0b
DEF TILE_NFAC EQU $0c
DEF TILE_GFAC EQU $0d

PUSHC

NEWCHARMAP Map

CHARMAP " ", TILE_OPEN
CHARMAP "+", TILE_ROAD
CHARMAP "o", TILE_HILL
CHARMAP "~", TILE_DSRT
CHARMAP "#", TILE_VLLY
CHARMAP "M", TILE_MNTN
CHARMAP "*", TILE_BRDG
CHARMAP "U", TILE_UBAS
CHARMAP "G", TILE_GBAS
CHARMAP "u", TILE_UFAC
CHARMAP "f", TILE_NFAC
CHARMAP "g", TILE_GFAC

CHARMAP "r", $0f
CHARMAP "s", $77
CHARMAP "h", $78
CHARMAP "i", $79
CHARMAP "j", $80
CHARMAP "k", $81
CHARMAP "l", $82
CHARMAP "q", $9c
CHARMAP "t", $da
CHARMAP "v", $20
CHARMAP "w", $bc
CHARMAP "x", $cd
CHARMAP "y", $13
CHARMAP "z", $e4

POPC

MACRO map
    db STRLEN(\1), _NARG
    PUSHC
    SETCHARMAP Map
    REPT _NARG
        db \1
        SHIFT
    ENDR
    POPC
ENDM
